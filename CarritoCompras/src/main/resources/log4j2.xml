<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %t %level %c %msg %n"/>
		</Console>
		<File fileName="LogGeneral.log" name="FileLogin">
			<PatternLayout pattern="%d %level %msg %n"/>
		</File>
	</Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="Console"/>
			<AppenderRef ref="FileLogin"/>
		</Root>
	</Loggers>
</Configuration>